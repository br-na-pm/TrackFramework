<?xml version="1.0" encoding="utf-8"?>
<EventBindingSet id="EMxAlarmContent_eventbinding" xmlns="http://www.br-automation.com/iat2014/eventbinding/v2" xmlns:types="http://www.br-automation.com/iat2015/widgetTypes/v2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Bindings>
    <EventBinding id="EMxAlarmContent.AcknButton.Click.1">
      <Source xsi:type="widgets.brease.Button.Event" contentRefId="EMxAlarmContent" widgetRefId="AcknButton" event="Click" />
      <EventHandler>
        <Action>
          <Target xsi:type="widgets.brease.AlarmList.Action" contentRefId="EMxAlarmContent" widgetRefId="AlarmList1">
            <Method xsi:type="widgets.brease.AlarmList.Action.Acknowledge" />
          </Target>
        </Action>
      </EventHandler>
    </EventBinding>
    <EventBinding id="EMxAlarmContent.AcknAllButton.Click.2">
      <Source xsi:type="widgets.brease.Button.Event" contentRefId="EMxAlarmContent" widgetRefId="AcknAllButton" event="Click" />
      <EventHandler>
        <Action>
          <Target xsi:type="widgets.brease.AlarmList.Action" contentRefId="EMxAlarmContent" widgetRefId="AlarmList1">
            <Method xsi:type="widgets.brease.AlarmList.Action.AcknowledgeAll" />
          </Target>
        </Action>
      </EventHandler>
    </EventBinding>
    <EventBinding id="EMxAlarmContent.SortDialog.Click">
      <Source xsi:type="widgets.brease.PushButton.Event" contentRefId="EMxAlarmContent" widgetRefId="SortDialog" event="Click" />
      <EventHandler>
        <Action>
          <Target xsi:type="widgets.brease.AlarmList.Action" contentRefId="EMxAlarmContent" widgetRefId="AlarmList1">
            <Method xsi:type="widgets.brease.AlarmList.Action.OpenConfiguration" type="Sorting" />
          </Target>
        </Action>
      </EventHandler>
    </EventBinding>
    <EventBinding id="EMxAlarmContent.FilterDialog.Click">
      <Source xsi:type="widgets.brease.PushButton.Event" contentRefId="EMxAlarmContent" widgetRefId="FilterDialog" event="Click" />
      <EventHandler>
        <Action>
          <Target xsi:type="widgets.brease.AlarmList.Action" contentRefId="EMxAlarmContent" widgetRefId="AlarmList1">
            <Method xsi:type="widgets.brease.AlarmList.Action.OpenConfiguration" type="Filtering" />
          </Target>
        </Action>
      </EventHandler>
    </EventBinding>
  </Bindings>
</EventBindingSet>